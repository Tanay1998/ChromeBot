{
    "name": "ChromeBot",
    "version": "1.0",
    "description": "Slash commands for Google Chrome. Powerup your browsing experience",
    "permissions": ["tabs", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "omnibox": { "keyword" : "/" },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
}